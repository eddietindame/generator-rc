{
  "name": "generator-tsnf",
  "version": "0.0.1",
  "description": "Generates a Typescript Node serverless function optimised for Vercel / Next.js",
  "homepage": "",
  "author": {
    "name": "eddietindame",
    "email": "eddie.tindame@googlemail.com",
    "url": "https://eddietindame.github.io"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "Next.js",
    "Vercel",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "eslint": "^6.0.1",
    "prettier": "^1.18.2",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-config-xo": "^0.26.0",
    "jest": "^24.8.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "eddietindame/generator-tsnf",
  "license": "Apache-2.0"
}
